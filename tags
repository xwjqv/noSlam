Blockstelle	Encoder.cpp	/^int Blockstelle = 0;$/;"	v
DBlock	Encoder.h	/^struct DBlock{$/;"	s
EncoL	Encoder.cpp	/^void EncoL (){$/;"	f
EncoR	Encoder.cpp	/^void EncoR (){$/;"	f
L	Encoder.h	/^	int L;$/;"	m	struct:encD
R	Encoder.h	/^	int R;$/;"	m	struct:encD
TC7_Handler	Encoder.cpp	/^void TC7_Handler()$/;"	f
TimerInit	Encoder.cpp	/^void TimerInit(){$/;"	f
currentBlock	Encoder.h	/^static DBlock * currentBlock;$/;"	v
enc	Encoder.h	/^	encD enc[encWerte];$/;"	m	struct:DBlock
encD	Encoder.h	/^struct encD {$/;"	s
encWerte	Encoder.h	/^const int encWerte = 510;$/;"	v
ptrN	Encoder.h	/^	DBlock * ptrN;$/;"	m	struct:DBlock
ptrP	Encoder.h	/^	DBlock * ptrP;$/;"	m	struct:DBlock
redptr	Encoder.cpp	/^encD * redptr ;$/;"	v
